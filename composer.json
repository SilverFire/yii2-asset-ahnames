{
    "name": "ahnames/yii2-asset-ahnames",
    "type": "yii2-extension",
    "description": "AHnames asset files: images, configs and more",
    "keywords": [
        "ahnames",
        "asset",
        "extension",
        "yii2"
    ],
    "homepage": "https://github.com/ahnames/yii2-asset-ahnames",
    "license": "proprietary",
    "support": {
        "email": "support@ahnames.com",
        "source": "https://github.com/ahnames/yii2-asset-ahnames",
        "issues": "https://github.com/ahnames/yii2-asset-ahnames/issues",
        "wiki": "https://github.com/ahnames/yii2-asset-ahnames/wiki"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "email": "sol@hiqdev.com",
            "homepage": "https://hipanel.com/"
        }
    ],
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "ahnames/hidev-vendor": "<2.0"
    },
    "extra": {
        "extension-plugin": {
            "hidev-config": "src/config/hidev.php",
            "hisite-config": "src/config/hisite.php"
        }
    },
    "autoload": {
        "psr-4": {
            "ahnames\\assets\\ahnames\\": "src"
        }
    }
}
